{
  "name": "angular-dual-listbox",
  "description": "Angular 2+ component for a dual listbox control.",
  "version": "4.3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/czeckd/angular-dual-listbox.git"
  },
  "bugs": {
    "url": "https://github.com/czeckd/angular-dual-listbox/issues"
  },
  "author": "David Czeck",
  "license": "MIT",
  "scripts": {
    "start": "rimraf runt && npm run dist && tsc && concurrently \"tsc -w\" \"lite-server\" ",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "clean": "rimraf runt tmp dist",
    "dist": "tools/mkdist",
    "lint": "tslint -c tslint.json -t verbose lib/**/*.ts app/**/*.ts || true",
    "lite": "lite-server"
  },
  "main": "angular-dual-listbox.bundle.js",
